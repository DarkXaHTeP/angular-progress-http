{
  "name": "angular-progress-http",
  "version": "0.3.0",
  "description": "A thin wrapper around Http that lets  track progress of http request in Angular 2",
  "main": "index.ts",
  "scripts": {
    "build": "npm run clean && npm run copy-ts && npm run copy-readme-license && npm run transform-package",
    "clean": "rimraf build/*",
    "test": "karma start --single-run",
    "test:watch": "karma start",
    "transform-package": "node scripts/transformPackageJson.js",
    "copy-readme-license": "copyfiles README.md LICENSE build",
    "copy-ts": "copyup \"src/**/*\" build",
    "build-examples": "webpack --config=./examples/webpack.config",
    "serve-examples": "node examples/server.js",
    "examples": "npm run build-examples && npm run serve-examples"
  },
  "keywords": [
    "angular",
    "angular2",
    "angular 2",
    "ng2",
    "http",
    "progress",
    "xhr",
    "XMLHttpRequest",
    "ajax",
    "upload"
  ],
  "author": "DarkXaHTeP <darkxahtep@gmail.com>",
  "license": "MIT",
  "repository": "DarkXaHTeP/angular-progress-http",
  "peerDependencies": {
    "@angular/core": "^2.3.1",
    "@angular/http": "^2.3.1",
    "rxjs": "^5.0.1"
  },
  "devDependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@types/jasmine": "^2.5.46",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "copyfiles": "^1.0.0",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "jasmine": "^2.5.3",
    "karma": "^1.5.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "multer": "^1.2.1",
    "phantomjs-prebuilt": "^2.1.14",
    "reflect-metadata": "^0.1.9",
    "rimraf": "^2.5.4",
    "rxjs": "^5.0.1",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "zone.js": "^0.7.2"
  }
}
